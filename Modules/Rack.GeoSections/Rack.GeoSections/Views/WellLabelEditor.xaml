<reactiveUi:ReactiveUserControl
    x:TypeArguments="model:WellLabel"
    x:Class="Rack.GeoSections.Views.WellLabelEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:model="clr-namespace:Rack.GeoSections.Model"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    Validation.ErrorTemplate="{x:Null}">
    <StackPanel>
        <ComboBox
            x:Name="WellComboBox"
            materialDesign:HintAssist.Hint="Скважина"
            Margin="5,5,5,15"/>
        <TextBox
            x:Name="TopTextBox"
            materialDesign:HintAssist.Hint="Верхняя граница"
            Margin="5,5,5,15" />
        <TextBox
            x:Name="BottomTextBox"
            materialDesign:HintAssist.Hint="Нижняя граница"
            Margin="5,5,5,15" />
        <TextBox
            x:Name="TextTextBox"
            AcceptsReturn="True"
            MaxLines="5"
            materialDesign:HintAssist.Hint="Текст"
            Margin="5,5,5,15" />
    </StackPanel>
</reactiveUi:ReactiveUserControl>